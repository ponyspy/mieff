.list_items
	each place in places
		.list_item
			a.item_rm.place(href='#', id=place._id) -- удалить
			a.item_title(href='/admin/places/edit/' + place._id, class=place.status)= (place.i18n.title.get('ru') || '!!! ПУСТОЕ ПОЛЕ !!!')

.list_counter
	a.list_back(href='#'): b назад
	span= ' - '
	select.list_select
		- var i = 0;
		while i < count
			option(value=i selected= (i == skip / 10) )= i+1
			- i++
	span= ' - '
	a.list_next(href='#'): b вперед
